    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "sass": "1.32.8",
    "sass-loader": "11.0.1",
    "standard": "16.0.3",
    "style-loader": "2.0.0",
    "webpack": "5.27.1",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*"
    ]
  }
}
