{
  "name": "ec2fieldday",
  "version": "1.0.0",
  "description": "This demo app is for the EC2 Field Day course. The app is called _Field Day_ and simulates field day events for a group of characters representing AWS Services.",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production --config ./client/webpack.config.js",
    "start-debug": "npm run start:client-debug && nodemon app.js",
    "start": "npm run build && npm run start:server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:client-debug": "webpack serve --mode development --config ./client/webpack.config.js",
    "start:server": "node app.js",
    "start:forever": "npx forever start -o out.log -e err.log app.js",
    "stop": "npx forever stop 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanmurakami/ec2fieldday.git"
  },
  "author": "Nabil S",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ryanmurakami/ec2fieldday/issues"
  },
  "homepage": "https://github.com/ryanmurakami/ec2fieldday#readme",
  "dependencies": {
    "@aws-sdk/client-config-service": "3.9.0",
    "@aws-sdk/signature-v4": "3.6.1",
    "aws-sdk": "2.868.0",
    "connect-redis": "5.1.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "flash": "1.1.0",
    "forever": "3.0.4",
    "lodash": "4.17.21",
    "node-fetch": "2.6.1",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "redis": "3.0.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "babel-loader": "8.2.2",
    "css-loader": "5.1.3",
    "dayjs": "1.10.4",
    "file-loader": "6.2.0",
    "grd": "1.4.0",
    "html-loader": "2.1.2",
