    "url": "https://github.com/ryanmurakami/ec2fieldday/issues"
  },
  "homepage": "https://github.com/ryanmurakami/ec2fieldday#readme",
  "dependencies": {
    "aws-sdk": "^2.782.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "winston": "^3.3.3"
  },
